				<simpleType>
					<restriction base="string">
						<pattern value="PL\.[A-Za-z]{1,6}\.\d{1,6}\.[A-Za-z0-9]{1,8}"/>
					</restriction>
				</simpleType>
			</element>
			<element name="wersjaId" type="dateTime" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>Identyfikator wersji obiektu.</documentation>
